# 生产环境配置
APP_ENV=production
API_BASE_URL=https://meme-manager.your-domain.workers.dev
LOG_LEVEL=warn

# 生产环境资源
D1_DATABASE_NAME=meme-manager-db-prod
R2_BUCKET_NAME=meme-manager-assets-prod
KV_NAMESPACE_NAME=meme-manager-cache-prod

# 生产环境特定配置
ENABLE_DEV_TOOLS=false
HOT_RELOAD=false
MOCK_DATA=false

# 生产环境安全配置
SECURE_HEADERS=true
CORS_STRICT=true